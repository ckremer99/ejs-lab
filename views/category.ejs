<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title><%= category %></title>
  </head>
  <body>
    <%- include('./partials/nav') %>
    <h1><%= category %></h1>
    <ul>
        <% menuItems.forEach((item, index) => { %>
            <li><%= item.name %> | <%= item.details %> | Rating: <%= item.rating %> | Price: <%= item.price %></li>
        <% }); %>
    </ul>
  </body>
</html>